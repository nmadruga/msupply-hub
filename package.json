{
  "name": "msupply-hub",
  "version": "0.0.0",
  "description": "Node.JS Express server with REST API, for consolidating information about all mSupply instances",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets babel-preset-env\"",
    "build": "babel src -s -D -d dist --presets babel-preset-env",
    "start": "node dist",
    "prestart": "npm run -s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sussol/msupply-information-server"
  },
  "author": "Sussol",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "jsonwebtoken": "^8.2.0",
    "morgan": "^1.8.0",
    "pg-promise": "^8.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.9.2"
  },
  "bugs": {
    "url": "https://github.com/sussol/msupply-information-server/issues"
  },
  "homepage": "https://github.com/sussol/msupply-information-server"
}
